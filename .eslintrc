{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "globals": {
    "forge": true
  },
  "rules": {
    "func-names": 0,
    "max-len": 0,
    "no-else-return": 0,
    "no-multiple-empty-lines": 0,
    "no-underscore-dangle": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0
  }
}
